-    /* Grab the global semaphore. */
-    gcmkONERROR(gckOS_AcquireSemaphore(
-        Hardware->os, Hardware->globalSemaphore
-        ));
-
-    globalAcquired = gcvTRUE;
-
-    gckHARDWARE_EnterQueryClock(Hardware, &mcStart, &shStart);
